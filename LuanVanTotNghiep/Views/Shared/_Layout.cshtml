    <!-- start menu -->
    <script src="~/Assets/client/js/simpleCart.min.js"></script>
    <!-- start menu -->
    <link href="~/Assets/client/css/memenu.css" rel="stylesheet" type="text/css" media="all" />
    <script type="text/javascript" src="~/Assets/client/js/memenu.js"></script>
    <script>
        $(document).ready(function () { $(".memenu").memenu(); });
    </script>
    <!-- /start menu -->
</head>
<body>
    <!--header-->
    <script src="~/Assets/client/js/responsiveslides.min.js"></script>
    <script>

        $(function () {
            $("#slider").responsiveSlides({
                auto: true,
                nav: true,
                speed: 500,
                namespace: "callbacks",
                pager: false,
            });
        });
    </script>
    <div class="header-top">
        <div class="header-bottom">
            <div class="logo">
                <h1><a href="/">Thành Công</a></h1>
            </div>
            <!---->
            <div class="top-nav">
                <ul class="memenu skyblue">
                    <li class="active"><a href="/">Trang chủ</a></li>
                    <li class="grid">
                        <a href="/home/about">Giới thiệu</a>
                    </li>
                    <li class="grid">
                        <a href="/sanpham">Thực đơn</a>
                        <div class="mepanel">
                            <div class="row">
                                @{
                                    Html.RenderAction("_categories", "sanpham");
                                }
                            </div>
                        </div>
                    </li>
                    <li class="grid"><a href="/datban/datban">Đặt bàn</a></li>
                </ul>
            </div>
            <!--checkout-->
            @Html.Action("GioHangPartial", "GioHang")

            <div class="clearfix"> </div>
            <!---->
        </div>
        <div class="clearfix"> </div>
    </div>
    <!-- END nav -->
    <!-- END section -->
    @RenderBody()


    @*@RenderPage("_Footer.cshtml")*@
    <!---->
    <div class="footer">
        <div class="container">
            <div class="footer-grids">
                @{
                    Html.RenderAction("_nhahang", "home");
                }
                <div class="col-md-3 ftr-grid">
                    <h3>Information</h3>
                    <ul class="nav-bottom">
                        <li><a href="#">Login</a></li>
                    </ul>
                </div>

            </div>
        </div>
    </div>
    <div class="copywrite">
        <div class="container">
            <div class="copy">
                <p>© 2015 Lighting. All Rights Reserved | Design by  <a href="http://w3layouts.com/" target="_blank">W3layouts</a> </p>
            </div>
            <div class="social">
                <a href="#"><i class="facebook"></i></a>
                <a href="#"><i class="twitter"></i></a>
                <a href="#"><i class="dribble"></i></a>
                <a href="#"><i class="google"></i></a>
                <a href="#"><i class="youtube"></i></a>
            </div>
            <div class="clearfix"></div>
        </div>
    </div>

    @RenderSection("scripts", required: false)
    @Html.Partial("_Toastr")
    <!---->
